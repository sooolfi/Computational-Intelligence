#!/usr/bin/env ruby
$LOAD_PATH.unshift File.expand_path('../../lib', __FILE__)

require 'ceie'

puts "\n" + "*" * 80  + "\n" +
     " " * 20 + "Enjambre de Particulas" + " " * 20 + "\n" +
     "*" * 80 + "\n"


rango = [[-512.0,512.0]]
enjambre = Ceie::Enjambre.new(40,rango,10,1,1)
enjambre.resolver

