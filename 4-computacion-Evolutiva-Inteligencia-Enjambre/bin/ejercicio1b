#!/usr/bin/env ruby
$LOAD_PATH.unshift File.expand_path('../../lib', __FILE__)

require 'ceie'

puts "\n" + "*" * 80  + "\n" +
     " " * 20 + "Algoritmo Genetico" + " " * 20 + "\n" +
     "*" * 80 + "\n"

genetica = Ceie::Genetic.new(20,21,200,2)
genetica.resolver

# Mejores parametros 
# 5 para numero - 16 para mantisa = 21 bits 
# estos parametros cambiarlos en genetic.rb

