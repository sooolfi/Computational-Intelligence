
#!/usr/bin/env ruby
$LOAD_PATH.unshift File.expand_path('../../lib', __FILE__)

require 'ceie'

puts "\n" + "*" * 80  + "\n" +
     " " * 20 + "Enjambre de Particulas" + " " * 20 + "\n" +
     "*" * 80 + "\n"


rangox = [0.0,20.0]
rangoy = [-10.0,20.0]
enjambre = Ceie::Enjambre.new(40,rangox,rangoy,10000,2)
enjambre.resolver


