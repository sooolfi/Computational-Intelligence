
#!/usr/bin/env ruby
$LOAD_PATH.unshift File.expand_path('../../lib', __FILE__)

require 'ceie'

puts "\n" + "*" * 80  + "\n" +
     " " * 20 + "Enjambre de Particulas" + " " * 20 + "\n" +
     "*" * 80 + "\n"


rango = [[-100.0,100.0],[-100.0,100.0]] 
enjambre = Ceie::Enjambre.new(50,rango,10000,3,2)
enjambre.resolver


